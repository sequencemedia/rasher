{
  "name": "rasher",
  "version": "1.0.171",
  "description": "A web browser DOM event listener and delegate management mechanism. DOM queries with native DOM methods or Sizzle. Supports current and legacy browsers (including IE7)",
  "main": "./index.js",
  "author": "Jonathan Perry for Sequence Media Limited <sequencemedia@sequencemedia.net>",
  "license": "MIT",
  "engines": {
    "node": ">=16.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/sequencemedia/rasher.git"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean",
    "clean": "rm -rf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "build": "npm run babel --",
    "build:watch": "npm run build -- -w",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "gulp": "gulp --color",
    "pre-commit": "npm run lint && npm run build && git add lib && npm run gulp -- pre-commit"
  },
  "dependencies": {
    "sizzle": "^2.3.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-export-default-from": "^7.17.12",
    "@babel/preset-env": "^7.18.2",
    "@babel/register": "^7.17.7",
    "babel-plugin-module-resolver": "^4.1.0",
    "core-js": "^3.22.7",
    "eslint": "^8.16.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "gulp": "^4.0.2",
    "husky": "^8.0.1"
  }
}
